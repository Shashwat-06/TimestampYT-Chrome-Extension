{
  "manifest_version": 3,
  "name": "Youtube-timestamp",
  "version": "1.0.0",
  "permissions": ["storage", "tabs"],
  "host_permissions": ["https://www.youtube.com/*"],
  "action": { "default_popup": "index.html" },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["contentScript.js"]
    }
  ]
}
